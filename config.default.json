{
  "app_env": "development",
  "port": "8080",
  "database": {
    "host": "localhost",
    "port": "5432",
    "user": "postgres",
    "dbname": "myapp_dev",
    "sslmode": "disable",
    "timezone": "UTC",
    "max_connections": 25,
    "timeout": "30s"
  },
  "storage": {
    "upload_dir": "./uploads",
    "processed_dir": "./processed",
    "export_dir": "./exports",
    "max_upload_size": 10485760,
    "allowed_extensions": [".jpg", ".png", ".pdf", ".csv"]
  },
  "rate_limit": {
    "requests_per_minute": 60,
    "burst": 10
  },
  "pagination": {
    "default_page_size": 20,
    "max_page_size": 100
  }
}